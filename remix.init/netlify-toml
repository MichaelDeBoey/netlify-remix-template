[build]
command = "npm run build"
publish = "public"

[dev]
command = "npm run dev"

[[redirects]]
from = "/*"
to = "/.netlify/functions/server"
status = 200

# Set immutable caching for static files, because they have fingerprinted filenames

[[headers]]
for = "/build/*"

[headers.values]

"Cache-Control" = "public, max-age=31560000, immutable"
